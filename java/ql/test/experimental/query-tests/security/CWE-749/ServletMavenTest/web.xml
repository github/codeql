<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>testServlet</servlet-name>
    <servlet-class>com.test.TestServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>testApiServlet</servlet-name>
    <servlet-class>com.online.TestApiServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>testMysqlServlet</servlet-name>
    <servlet-class>com.online.TestMysqlServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>testServlet</servlet-name>
    <url-pattern>/testServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>testApiServlet</servlet-name>
    <url-pattern>/testApiServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>testMysqlServlet</servlet-name>
    <url-pattern>/testMysqlServlet</url-pattern>
  </servlet-mapping>
</web-app>
