{
    "name": "typescript-parser-wrapper",
    "private": true,
    "dependencies": {
        "typescript": "^5.7.2"
    },
    "scripts": {
        "build": "tsc --project tsconfig.json",
        "check": "tsc --noEmit --project . && tslint --project .",
        "lint": "tslint --project .",
        "lint-fix": "tslint --project . --fix",
        "watch": "tsc -p . -w --sourceMap"
    },
    "devDependencies": {
        "@types/node": "18.15.3"
    }
}
